#include <iostream>
using namespace std;

int main(void)
{
	int auxiliary;
	int vector1[7] = {4, 7, 2, 8, 1, 3, 0};

	int vector2[7] = {0};

	for(int i = 0; i < 7; i++)
	{
		auxiliary = vector1[i];
		vector1[i] = vector2[i+1];
		vector2[i+1] = auxiliary;


				cout << vector2[i] << ' ';
	}
	cout << endl;

	return 0;
}
